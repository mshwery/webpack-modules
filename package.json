{
  "name": "fulcrum-modules",
  "version": "0.0.0",
  "description": "Monorepo for managing Fulcrum modules",
  "private": true,
  "scripts": {
    "start": "npm run clean && npm run build && npm run open",
    "clean": "rimraf build",
    "build": "npm run build:modules && npm run build:spa",
    "build:modules": "webpack --progress",
    "build:spa": "webpack --config webpack.spa.js --progress",
    "open": "open build/index.html"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.1.2",
    "html-webpack-plugin": "^2.22.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0"
  }
}
